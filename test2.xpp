  arr img1 = load("./img2.png");
  arr img2 = load("./img2.png");
  arr img3 = collage(img1, img2);
  save(img3, "./img3_collaged.png");
  close(img1, 0);
  close(img2, 0);

  arr img4 = load("./img2.png");
  arr img5 = crop(img4, 50, 50, 100, 100);
  save(img5, "./img2_cropped.png");
  close(img4, 0);

  arr img6 = load("./img2.png");
  arr img7 = flip(img6);
  save(img7, "./img2_flipped.png");
  close(img6, 0);

  return 0;
}
